<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/check_string_relative_layout" android:focusable="false">
        <CheckBox android:layout_height="wrap_content" android:text="CheckBox" android:layout_width="wrap_content" android:layout_alignParentTop="true" android:layout_alignParentRight="true" android:id="@+id/check_string_box" android:layout_marginRight="5dp" android:clickable="false" android:focusable="false"></CheckBox>
        <TextView android:layout_height="wrap_content" android:text="TextView" android:textAppearance="?android:attr/textAppearanceLarge" android:layout_width="wrap_content" android:layout_centerVertical="true" android:layout_alignParentLeft="true" android:layout_marginLeft="5dp" android:id="@+id/check_string_text"></TextView>
    </RelativeLayout>
    
</LinearLayout>
